{
  "buildCommand": null,
  "outputDirectory": "docs",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Content-Language",
          "value": "zh-CN"
        },
        {
          "key": "Vary",
          "value": "Accept-Language"
        }
      ]
    },
    {
      "source": "/index-en.html",
      "headers": [
        {
          "key": "Content-Language",
          "value": "en"
        },
        {
          "key": "Vary",
          "value": "Accept-Language"
        }
      ]
    }
  ]
}

